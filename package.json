{
  "name": "al-development-collection",
  "version": "2.9.0",
  "description": "AL Development Collection for GitHub Copilot - AI Native-Instructions Architecture with intelligent installation (auto-detects AL projects, validates setup, smart updates)",
  "main": "install.js",
  "bin": {
    "al-collection": "./install.js"
  },
  "scripts": {
    "validate": "node validate-al-collection.js",
    "test": "node validate-al-collection.js",
    "install-toolkit": "node install.js install",
    "update-toolkit": "node install.js update",
    "validate-installation": "node install.js validate",
    "postinstall": "echo '\n✨ AL Development Collection installed!\n\nCommands available:\n  npx al-collection install   - Install to your project\n  npx al-collection update    - Update existing installation\n  npx al-collection validate  - Verify installation\n  npx al-collection --help    - Show full help\n'"
  },
  "files": [
    "agents",
    "instructions",
    "prompts",
    "install.js",
    "validate-al-collection.js",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "al",
    "business-central",
    "dynamics365",
    "github-copilot",
    "collection",
    "ai-native",
    "instructions-architecture",
    "agent-primitives",
    "copilot-extension"
  ],
